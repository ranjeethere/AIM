// two sum
#include <iostream>  
  
int addNumbers(int a, int b) {  
  int result;  
  result = a + b;  
  return result;  
}  
  
int main() {  
  int num1, num2, sum;  
  std::cout << "Enter two numbers: ";  
  std::cin >> num1 >> num2;  
  sum = addNumbers(num1, num2);  
  std::cout << "The sum of " << num1 << " and " << num2 << " is: " << sum << std::endl;  
  return 0;  
}  e
